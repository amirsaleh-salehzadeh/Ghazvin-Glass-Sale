<%@ page language="java" import="java.util.*" pageEncoding="utf-8"%>

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
  <head>
    <link rel="shortcut icon" href="images/logoSmallFa.png">
    <title>صنایع شیشه ای قزوین</title>
    <script type="text/javascript" src="js/jquery-1.4.2.js"></script>
    <style type="text/css">
    	td {
    		text-align: center;
    		color: #005387;
    		font-weight: bold;
    		font-size: 18px;
    	}
    
    </style>
  </head>
  <body>
  	<br/><br/><br/>
	<table width="680" border="0" align="center" cellpadding="0" cellspacing="0">
		<tr>
			<td>
				<img src="images/logoFa.png"></img>
			</td>
		</tr>
		<tr>
			<td class="language">
				<img src="images/welcomePage/farsi0.png" style="cursor:pointer;" onmouseover="$(this).attr('src','images/welcomePage/farsi1.png')" onmouseout="$(this).attr('src','images/welcomePage/farsi0.png')" onclick="window.location.replace('t_requestForPage.do?reqCode=farsi');"></img>
			</td>
		</tr>
		<tr>
			<td class="language">
				<img src="images/welcomePage/english0.png" style="cursor:pointer;" onmouseover="$(this).attr('src','images/welcomePage/english1.png')" onmouseout="$(this).attr('src','images/welcomePage/english0.png')" onclick="window.location.replace('t_requestForPage.do?reqCode=english');"></img>
			</td>
		</tr>
		<tr>
			<td class="language">
				<img src="images/welcomePage/arabic0.png" style="cursor:pointer;" onmouseover="$(this).attr('src','images/welcomePage/arabic1.png')"  onmouseout="$(this).attr('src','images/welcomePage/arabic0.png')"></img>
			</td>
		</tr>
	</table>
  </body>
</html>
