<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ams.ggc.common.order.OrderDetailENT" table="orders_detail" >
        <id name="orderDetailId" type="java.lang.Integer">
            <column name="order_detailID" />
            <generator class="increment" />
        </id>
		<property name="Price" type="java.lang.String">
            <column name="price"/>
        </property>
        <property name="amount" type="java.lang.String">
            <column name="amount"/>
        </property>
        <property name="productId" type="java.lang.Integer">
            <column name="productID"/>
        </property>
        <property name="orderId" type="java.lang.Integer">
            <column name="orderID"/>
        </property>
        <property name="PackingPrice" type="java.lang.String">
            <column name="packingPrice"/>
        </property>
	</class>
</hibernate-mapping>
